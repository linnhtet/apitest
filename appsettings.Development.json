{
  // NOTE: When in debugging mode, appsettings.json is applied first, then this file will overwrite the Overlapped settings sections!
  // use this setting (for debugging) will generate more ASP logs than appsetting.json (Production setting)
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Information"
    }
  }
}
